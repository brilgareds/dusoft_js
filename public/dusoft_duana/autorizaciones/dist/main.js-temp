            that.buscarProductosBloqueados = function(termino,paginando) {
                
                 if ($scope.ultima_busqueda !== $scope.termino) {
                    $scope.paginaactual = 1;
                }
                
                var obj = {
                    termino_busqueda: $scope.termino,
                    pagina_actual: $scope.paginaactual,
