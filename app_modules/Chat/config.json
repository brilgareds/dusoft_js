{
    "module": "Chat",
    "dependency": {
        "controllers": [{
                "chat_controller": {
                    "module": "Chat",
                    "controller": "ChatController",
                    "alias": "c_chat"
                }
            }],
        "models": [{
                "chat_model": {
                    "module": "Chat",
                    "model": "ChatModel",
                    "alias": "m_chat"
                }
            }],
        "events": [{
                "auth_events": {
                    "module": "Chat",
                    "event": "ChatEvents",
                    "alias": "e_chat"
                }
            }],
        "cronJobs": []
    }
}
