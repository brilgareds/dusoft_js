                $timeout(function(){
                    
                    self.consultarGrupos(function(valido){
                        if(valido){
                            self.listarUsuariosConversacion(function(){
                  
                            });
                        } else {
                            AlertService.mostrarVentanaAlerta("Mensaje del sistema", "Ha ocurrido un error consultando los grupos");
